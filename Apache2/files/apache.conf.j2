<VirtualHost *:80>
   ServerAdmin webmaster@192.168.56.6
   ServerName {{ http_host }}
   ServerAlias www.{{ http_host }}
   DocumentRoot /var/www/{{ http_conf }}/public
   <Directory /var/www/{{ http_conf }}/public>
    AllowOverRide All
   </Directory>
   ErrorLog ${APACHE_LOG_DIR}/error.log
   CustomLog ${APACHE_LOG_DIR}/access.log combined
</VirtualHost>
